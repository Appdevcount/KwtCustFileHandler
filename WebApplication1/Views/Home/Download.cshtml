
@model List<string>

<h2>Downloads</h2>
<table>
    <tr>
        <th>File Name</th>
        <th>Link</th>
    </tr>
    @for (var i = 0; i <= Model.Count - 1; i++)
    {
        <tr>
            <td>

      @Model[i].ToString()
            </td>
            <td>

         

                @Html.ActionLink(@Model[i].ToString(), "DownloadFile", "Home", new { filename = @Model[i].ToString(),@userid='2'  }, null)


                <td>

                   
                </td>



            </td>
        </tr>
    }
</table>