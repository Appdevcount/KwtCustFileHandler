@model WebApplication1.Models.FileModel
@using System
@{
    ViewBag.Title = "CommercialView";
}

<!DOCTYPE html>

<html>
<head>
    <title>CommericalView</title>


    <style type="text/css">
        table, th, td {
            margin: 10px 0;
            border: solid 1px #333;
            padding: 2px 4px;
            font: 15px Verdana;
        }

        th {
            font-weight: bold;
        }
    </style>

</head>
<body>
    @if(Model.disabled!='true')
    {
    <div> 


        <input type="button" onclick="documentslist()" value="Data" />
        <div id="DocumentsData"></div>

    </div>
    }
</body>

</html>

<script src="~/Scripts/DocumentsData.js"></script>
<script type="text/javascript">
    function documentslist() 
    {
        //header
        var col = [];
        for (var i = 0; i < documentsdata.length; i++) {
            for (var key in documentsdata[i]) {
                if (col.indexOf(key) === -1) {
                    col.push(key);
                }
            }
        }
        
        var table = document.createElement("table");
        
        var tr = table.insertRow(-1);                   

        for (var i = 0; i < col.length; i++) {
            var th = document.createElement("th");      
            th.innerHTML = col[i];
            tr.appendChild(th);
        }

        for (var i = 0; i < documentsdata.length; i++) {

            tr = table.insertRow(-1);

            for (var j = 0; j < col.length; j++) {
                var tabCell = tr.insertCell(-1);
                  tabCell.innerHTML = documentsdata[i][col[j]];
              //  tabCell.innerHTML ='<input type="checkbox" id='+'

             //   node.innerHTML = '<input type="checkbox" id="check' + i + '" name="check' + i + '"><label for="check' + i + '">' + title + '</label>';


            }
        }

           var divContainer = document.getElementById("DocumentsData");
        divContainer.innerHTML = "";
        divContainer.appendChild(table);
        return false;
    }

</script>